{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",

  "rewrites": [
    { "source": "/api/:path*", "destination": "https://flexidesk-api.onrender.com/api/:path*" },
    { "source": "/((?!assets|favicon\\.ico|robots\\.txt|manifest\\.webmanifest).*)", "destination": "/" }
  ]
}
